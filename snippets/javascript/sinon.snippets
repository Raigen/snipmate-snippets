snippet fserver
    this.server = sinon.fakeServer.create();${1}

snippet fsrw
    this.server.respondWith('${1:POST}', '${2:path}', [${3:200}, {
        'Content-Type': '${4:text/html}'
    }, '${5:response}']);${6}

snippet resp
    this.server.respond();

snippet rest
    this.server.restore();

snippet sstub
    sinon.stub({$1});

snippet sspy
    sinon.spy({$1});

snippet sstubr
    sinon.stub(${1}).returns(${2:true});
